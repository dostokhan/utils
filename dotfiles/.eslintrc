{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "indent": ["error", 2, {"SwitchCase": 1}],
        "no-console": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/forbid-prop-types": ['error', { "forbid": ['any'] }],
        "class-methods-use-this": "off",
        'jsx-a11y/no-static-element-interactions': [
          'warn',
          {
            handlers: [
              'onClick',
              'onMouseDown',
              'onMouseUp',
              'onKeyPress',
              'onKeyDown',
              'onKeyUp',
              'onMouseOver',
            ],
          },
        ],
    } 
}
